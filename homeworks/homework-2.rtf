//Additional Ex 2\
function fibonacciNumber(numberGiven) \{\
    var a = -1; \
    var b = 1; \
    var fibonacci;\
    var fibonacciCollection = [];\
  \
    for(var i = 1; i <= numberGiven; i++) \{\
        fibonacci = a + b;\
        a = b;\
        b = fibonacci;\
        fibonacciCollection.push(fibonacci);\
    \}\
    return fibonacciCollection;\
\}\
\
console.log(fibonacciNumber(100));\
