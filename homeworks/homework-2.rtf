function palidrome(word) \{\
  var wordToArray = word.split('');\
  var wordToArrayReversed = word.split('').reverse();\
  var palidrome;\
  \
  for(var i = 0; i < wordToArray.length; i++) \{\
     \
    if (wordToArray[i] === wordToArrayReversed[i]) \{\
      palidrome = true;\
    \}\
    \
  \}\
  \
  if(palidrome)\{\
    return  'The word ' + word + ' is a palidrome';\
  \} \
  \
\}\
\
console.log(palidrome('kayak'));\
